{
  "opts": {
    "destination": "./docs",
    "template": "node_modules/@sugarcrm/jsdoc-baseline"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "plugins": ["transform-class-properties"],
    "babelrc": false
  },
  "templates": {
    "baseline": "baseline-config.json"
  }
}
